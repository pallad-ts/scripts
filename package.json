{
  "name": "@pallad/scripts",
  "version": "3.0.0-alpha.1",
  "description": "Development scripts for @pallad and alpha-* packages",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "compile": "rm -rf compiled && tsc --incremental --project src",
    "lint": "eslint src/**/*.ts",
    "prepack": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pallad-ts/scripts.git"
  },
  "author": "Łukasz Kużyński <lukasz.kuzynski@gmail.com> (http://wookieb.pl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pallad-ts/scripts/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bin": {
    "pallad-scripts": "./bin/run"
  },
  "types": "compiled/index.d.ts",
  "oclif": {
    "commands": "./compiled/commands",
    "bin": "pallad-scripts",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [
    "/bin",
    "/compiled",
    "/oclif.manifest.json",
    "/templates",
    "/dist",
    "/jest-preset.js"
  ],
  "homepage": "https://github.com/wookieb/alpha-scripts#readme",
  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.3",
    "@oclif/plugin-help": "^5",
    "@pallad/eslint-config": "^1.1.1",
    "@types/jest": "^29",
    "@types/sinon": "^10",
    "eslint": "^8",
    "jest": "^29",
    "jest-junit": "^14",
    "resolve-bin": "^1",
    "ts-jest": "^29",
    "tslib": "^2",
    "typescript": "^4"
  },
  "peerDependencies": {
    "sinon": "^14"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "globby": "^13",
    "ts-node": "^10"
  }
}

{
  "name": "@pallad/scripts",
  "version": "2.1.0",
  "description": "Development scripts for alpha-* packages",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "compile": "rm -rf compiled && tsc --incremental --project src",
    "lint": "eslint src/**/*.ts",
    "prepack": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pallad-ts/scripts.git"
  },
  "author": "Łukasz Kużyński <lukasz.kuzynski@gmail.com> (http://wookieb.pl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pallad-ts/scripts/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "bin": {
    "pallad-scripts": "./bin/run"
  },
  "types": "compiled/index.d.ts",
  "oclif": {
    "commands": "./compiled/commands",
    "bin": "pallad-scripts",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [
    "/bin",
    "/compiled",
    "/oclif.manifest.json",
    "/templates",
    "/dist",
    "/jest-preset.js"
  ],
  "homepage": "https://github.com/wookieb/alpha-scripts#readme",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@pallad/eslint-config": "^1",
    "@types/jest": "^27",
    "@types/sinon": "^9",
    "eslint": "^7.32.0",
    "jest": "^27",
    "resolve-bin": "^0.4.3",
    "ts-jest": "^27",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "sinon": "^9"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "globby": "^10",
    "ts-node": "^9.1.1"
  }
}

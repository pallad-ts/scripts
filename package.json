{
  "name": "alpha-scripts",
  "version": "1.0.0",
  "description": "Development scripts for alpha-* packages",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "postpack": "rm -f oclif.manifest.json",
    "compile": "rm -rf compiled && tsc -b",
    "prepack": "npm run compile && oclif-dev manifest && oclif-dev readme"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wookieb/alpha-scripts.git"
  },
  "author": "Łukasz Kużyński <lukasz.kuzynski@gmail.com> (http://wookieb.pl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wookieb/alpha-scripts/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "bin": {
    "alpha-scripts": "./bin/run"
  },
  "types": "compiled/index.d.ts",
  "oclif": {
    "commands": "./compiled/commands",
    "bin": "alpha-scripts",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [
    "/bin",
    "/compiled",
    "/package-lock.json",
    "/oclif.manifest.json",
    "/configs",
    "/templates"
  ],
  "homepage": "https://github.com/wookieb/alpha-scripts#readme",
  "dependencies": {
    "@oclif/command": "^1.5.17",
    "@oclif/config": "^1.13.2",
    "@oclif/plugin-help": "^2.2.0",
    "@types/jest": "^24.0.15",
    "@types/sinon": "^7.0.13",
    "jest": "^24.8.0",
    "sinon": "^7.3.2",
    "ts-jest": "^24.0.2",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "globby": "^10.0.1",
    "ts-node": "^8.3.0"
  }
}

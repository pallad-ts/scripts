{
  "name": "@pallad/scripts",
  "version": "1.2.3",
  "description": "Development scripts for alpha-* packages",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "postpack": "rm -f oclif.manifest.json",
    "compile": "rm -rf compiled && tsc -b",
    "prepack": "npm run compile && oclif-dev manifest && oclif-dev readme"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pallad-ts/scripts.git"
  },
  "author": "Łukasz Kużyński <lukasz.kuzynski@gmail.com> (http://wookieb.pl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pallad-ts/scripts/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "bin": {
    "pallad-scripts": "./bin/run"
  },
  "types": "compiled/index.d.ts",
  "oclif": {
    "commands": "./compiled/commands",
    "bin": "pallad-scripts",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [
    "/bin",
    "/compiled",
    "/package-lock.json",
    "/oclif.manifest.json",
    "/templates",
    "/tslint.dist.json",
    "/tsconfig-node.dist.json",
    "/jest-preset.js"
  ],
  "homepage": "https://github.com/wookieb/alpha-scripts#readme",
  "dependencies": {
    "@darkobits/publish-root": "^1.0.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/jest": "^26",
    "@types/sinon": "^9",
    "jest": "^26",
    "resolve-bin": "^0.4.0",
    "ts-jest": "^26",
    "tslib": "^2",
    "tslint": "^5",
    "typescript": "^4"
  },
  "peerDependencies": {
    "sinon": "^9"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "globby": "^10",
    "ts-node": "^9"
  }
}
